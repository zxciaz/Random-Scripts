local Players, Uis, RService, RStorage, LStorage = game:GetService("Players"), game:GetService("UserInputService"), game:GetService("RunService"), game:GetService("ReplicatedStorage"), game:GetService("Lighting")
local Client, Mouse, Camera, INew = Players.LocalPlayer, Players.LocalPlayer:GetMouse(), workspace.CurrentCamera, Instance.new;
local AntiFe, GLaunch, BLighting = false, true, false;
local FOV, ResetKey, TTalkKey = 70, "R", "G";
local TrashTalk = {"you suck LOL", "your trash", "my nan can play better than you LOL", "ur mad", "NEXT"};

getgenv().Notify = function(title, text, icon, time)
    game:GetService("StarterGui"):SetCore("SendNotification",{
        Title = title;
        Text = text;
        Icon = "rbxassetid://5793181157";
        Duration = time;
    }) 
end
getgenv().ChatWord = function(string)
    RStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(tostring(string), "All")
end
local function PrintCommands()
    if rconsoleprint then 
        rconsolename("Citizen Legit Mode")
        rconsoleprint([[
        
        Directions:
        
        Type in a Letter/Number for the type of box then press ENTER to confirm
        idrgaf about this so dont ask me to update it 
        if your going to use something from this give me credits
        
        ]])
    else 
        print([[
        
        Citizen Legit Mode
        Directions:
        
        Type in a Letter/Number for the type of box then press ENTER to confirm
        idrgaf about this so dont ask me to update it 
        if your going to use something from this give me credits
        
        ]])
    end
end
PrintCommands()
local function ChatSpy()
    local ChatSpyFrame = Client.PlayerGui.Chat.Frame
    ChatSpyFrame.ChatChannelParentFrame.Visible = true
    ChatSpyFrame.ChatBarParentFrame.Position = ChatSpyFrame.ChatChannelParentFrame.Position + UDim2.new(UDim.new(), ChatSpyFrame.ChatChannelParentFrame.Size.Y)
end
ChatSpy()

Uis.InputBegan:Connect(function(Key)
    if not Uis:GetFocusedTextBox() then 
        if Key.KeyCode == Enum.KeyCode[ResetKey] then 
            Client.Character:FindFirstChild("Humanoid"):ChangeState(15)
        end
        if Key.KeyCode == Enum.KeyCode[TTalkKey] then 
            ChatWord(TrashTalk[math.random(1, #TrashTalk)])
        end
    end
end)

RService.Stepped:Connect(function()
    if GLaunch == true then 
        pcall(function()
            if Client and Client.Backpack and Client.Character then 
                Client.Character:FindFirstChild("Glock").CanCollide = true 
            else 
                Client.Backpack:FindFirstChild("Glock").CanCollide = true
            end 
        end)
    elseif GLaunch == false then 
        pcall(function()
            if Client and Client.Backpack and Client.Character then
                Client.Character:FindFirstChild("Glock").CanCollide = false 
            else 
                Client.Backpack:FindFirstChild("Glock").CanCollide = false
            end
        end)
    end
end)

FieldOfViewBox.FocusLost:Connect(function(F1)
    if F1 == true then 
        FOV = FieldOfViewBox.Text 
        Camera.FieldOfView = FOV 
    end
    Notify("Citizen", "FOV: "..tonumber(FOV), "", 3)
end)
ResetKeyBox.FocusLost:Connect(function(F2)
    if F2 == true then 
        ResetKeyBox.Text = string.upper(ResetKeyBox.Text)
        ResetKey = string.upper(ResetKeyBox.Text)
    end
    Notify("Citizen", "Reset Key: "..tostring(ResetKey), "", 3)
end)
TrashTalkKeyBox.FocusLost:Connect(function(F3)
    if F3 == true then 
        TrashTalkKeyBox.Text = string.upper(TrashTalkKeyBox.Text)
        TTalkKey = string.upper(TrashTalkKeyBox.Text)
    end
    Notify("Citizen", "Trash-Talk Key: "..tostring(TTalkKey), "", 3)
end)

BetterLightingButton.MouseButton1Click:Connect(function()
    BLighting = not BLighting
    if BLighting == true and LStorage:FindFirstChild("ColorCorrection") and LStorage:FindFirstChild("Sky") then 
        LStorage["ColorCorrection"]:Destroy()
        LStorage["Sky"]:Destroy()
    end
    Notify("Citizen", "Better Lighting: "..tostring(BLighting), "", 3)
end)
GlockLaunchButton.MouseButton1Click:Connect(function()
    GLaunch = not GLaunch
    Notify("Citizen", "Glock Launch: "..tostring(GLaunch), "", 3)
end)
AntiFeButton.MouseButton1Click:Connect(function()
    AntiFe = not AntiFe
    Notify("Citizen", "Anti Fe: "..tostring(AntiFe), "", 3)
    if AntiFe == true then 
        for _, v in next, Client.Backpack:GetChildren() do 
            if v:IsA("Tool") then 
                local TFolder = INew("Folder", v.Handle)
                TFolder.Name = "TFolder"
            end
            Client.Character.ChildAdded:Connect(function(t1)
                if not t1.Handle:FindFirstChild("TFolder") then 
                    t1:Destroy()
                end
            end)
            Client.Backpack.ChildAdded:Connect(function(t2)
                if not t2.Handle:FindFirstChild("TFolder") then 
                    t2:Destroy()
                end
            end)
        end
    elseif AntiFe == false then return end
end)
