Commands["Buy Item"] = {
    ["Aliases"] = {"get"};
    ["Function"] = function(args)
        if GunTable[args[1]] then 
            local Weapon;Weapon = GunTable[args[1]]
            local Root = Client.Character:FindFirstChild("HumanoidRootPart") or Client.Character:FindFirstChild("Torso")
            local OldPos = Root.Position;Teleporting = true
            if workspace:FindFirstChild(Weapon) then 
                repeat Root.CFrame = Root.CFrame * CF(math.random(0.1, 900),0, math.random(0.1, 900))
                    while Teleporting == true and RService.Heartbeat:Wait() do 
                        Root.CFrame = workspace[Weapon]:FindFirstChildOfClass("Part").CFrame + Vec3(0.1, 1.1, 0.1)
                    end
                until Client.Backpack.Uzi or Client.Backpack.Glock or Client.Backpack.SawedOff or Client.Backpack.Burger or Client.Backpack.Pipe or Client.Backpack.Chicken
                Root = OldPos;Teleporting = false 
            end
        end
    end
}

