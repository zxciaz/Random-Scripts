                        while GKTarget and GKTarget.Character and GKTarget.Character:FindFirstChild(AimPart) and Glock and RP do 
                            Client.Character["Right Arm"]:FindFirstChild("RightGrip"):Destroy()
                            repeat RService.Heartbeat:Wait() 
                                RP.Position = GKTarget.Character.Head.Position
                            until GKTarget.Character:FindFirstChild("Bone", true) or GKTarget.Character:FindFirstChild("KO") or GKTarget.Character.Humanoid.Health == 0 or Client.Character:FindFirstChild("Bone", true) or Client.Character:FindFirstChild("KO") or Client.Character.Humanoid.Health == 0
                            if GKTarget.Character:FindFirstChild("Bone", true) or GKTarget.Character:FindFirstChild("KO") or GKTarget.Character.Humanoid.Health == 0 or Client.Character:FindFirstChild("Bone", true) or Client.Character:FindFirstChild("KO") or Client.Character.Humanoid.Health == 0 then 
                                GKTarget = nil;RP:Destroy()
                            end
                        end
                    end
                end
            end
        Notify("Citizen", "Glock-Follow Target: "..tostring(GKTarget), "", 3)
    end
}
