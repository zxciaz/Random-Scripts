local FileName = "CitizenV3Settings.json"
if writefile and readfile then 
    local ExistingFile = pcall(readfile, FileName)
    if not ExistingFile then 
        SetKeyBinds(FileName)
    else 
        RunKeyBinds(FileName)
    end
end

getgenv().ChatWord = function(string)
    RStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(tostring(string), "All")
end

getgenv().ResetCharacter = function()
    if Client and Client.Character and Client.Character:FindFirstChild("Humanoid") then 
        Client.Character.Humanoid:ChangeState(15)
    end
end

getgenv().PShootTarget = function(Plr)
    local Tool = Client.Character:FindFirstChildWhichIsA("Tool")
    if Plr and Plr.Character and Plr.Character:FindFirstChild(AimPart) and not Plr.Character:FindFirstChild("ForceField") and Tool then 
        Tool.Fire:FireServer(CF(AimlockTarget.Character[AimPart].Position) + AimlockTarget.Character[AimPart].Velocity/AV + AimlockTarget.Character[AimPart].RotVelocity/AV)
    end
end

getgenv().MBotPlayer = function(Player)
    if Client and Client.Character and Client.Character:FindFirstChildWhichIsA("Tool") then 
        local TFound = Client.Character:FindFirstChildWhichIsA("Tool")
        local ClientPF = Client.Character:FindFirstChild("HumanoidRootPart") or Client.Character:FindFirstChild("Torso")
        if Player and Player.Character and Player.Character:FindFirstChild(AimPart) and ClientPF and TFound then 
            ClientPF.CFrame = Player.Character[AimPart].CFrame * CF(math.random(1, 7),5, math.random(-7, 1))
        end
    end
end

getgenv().Teleport = function(Thing)
    if Client and Client.Character then 
        local PFound = Client.Character:FindFirstChild("HumanoidRootPart") or Client.Character:FindFirstChild("Torso")
        if PFound and Thing then 
            local TPAction = TweenService:Create(PFound, TweenInfo.new(
                TPSpeed + .1, 
                Enum.EasingStyle.Sine, 
                Enum.EasingDirection.InOut), 
                {CFrame = Thing}
            )
            TPAction:Play()
        end
    end
end

getgenv().togglefly = function()
    Flying = not Flying
    Notify("Citizen", "Flying: "..tostring(Flying), "", 3)
    local T = Client.Character:FindFirstChild("HumanoidRootPart") or Client.Character:FindFirstChild("Torso")
    local BV, BG = INew("BodyVelocity", T), INew("BodyGyro", T)
    BV.Velocity = Vec3(0, 0.1, 0);BV.MaxForce = Vec3(math.huge, math.huge, math.huge)
    BG.CFrame = T.CFrame;BG.P = 9e9;BG.MaxTorque = Vec3(9e9, 9e9, 9e9)
    local FlyPart = INew("Part", workspace)
    FlyPart.Anchored = true;FlyPart.Size = Vec3(6, 1, 6);FlyPart.Transparency = 1 
    
    while Flying == true and Client and Client.Character and Client.Character:FindFirstChild("Humanoid") and Client.Character.Humanoid.Health ~= 0 and RService.Heartbeat:Wait() and T do 
        local Front, Back, Left, Right = 0, 0, 0, 0
        if KeysTable["W"] then 
            Front = FSpeed 
        elseif not KeysTable["W"] then
            Front = 0 
        end
        if KeysTable["A"] then 
            Right = -FSpeed
        elseif not KeysTable["A"] then 
            Right = 0 
        end
        if KeysTable["S"] then 
            Back = -FSpeed 
        elseif not KeysTable["S"] then 
            Back = 0
        end
        if KeysTable["D"] then 
            Left = FSpeed
        elseif not KeysTable["D"] then 
            Left = 0
        end
        if tonumber(Front + Back) ~= 0 or tonumber(Left + Right) ~= 0 then 
            BV.Velocity = ((Camera.CoordinateFrame.lookVector * (Front + Back)) + ((Camera.CoordinateFrame * CF(Left + Right, (Front + Back) * 0.2, 0).p) - Camera.CoordinateFrame.p)) * 50
        else 
            BV.Velocity = Vec3(0, 0.1, 0)
        end
        BG.CFrame = Camera.CoordinateFrame
    end
    FlyPart:Destroy();BG:Remove();BV:Remove()
end

getgenv().CreatePartEsp = function(Player)
    if Player and Player.Character and Player.Character:FindFirstChild(AimPart) then 
        -- Torso
        local SBox1 = INew("SelectionBox", workspace)
        SBox1.LineThickness = 0.001
        SBox1.Transparency = 0.1
        SBox1.Color3 = Color3.new(245,245,245)
        SBox1.Adornee = Player.Character:FindFirstChild("Torso")
        SBox1.Parent = Player.Character:FindFirstChild("Torso")
        -- Right Arm
        local SBox2 = INew("SelectionBox", workspace)
        SBox2.LineThickness = 0.001
        SBox2.Transparency = 0.1
        SBox2.Color3 = Color3.new(245,245,245)
        SBox2.Adornee = Player.Character:FindFirstChild("Right Arm")
        SBox2.Parent = Player.Character:FindFirstChild("Right Arm")
        -- Left Arm 
        local SBox3 = INew("SelectionBox", workspace)
        SBox3.LineThickness = 0.001
        SBox3.Transparency = 0.1
        SBox3.Color3 = Color3.new(245,245,245)
        SBox3.Adornee = Player.Character:FindFirstChild("Left Arm")
        SBox3.Parent = Player.Character:FindFirstChild("Left Arm")
        -- Right Leg 
        local SBox4 = INew("SelectionBox", workspace)
        SBox4.LineThickness = 0.001
        SBox4.Transparency = 0.1
        SBox4.Color3 = Color3.new(245,245,245)
        SBox4.Adornee = Player.Character:FindFirstChild("Right Leg")
        SBox4.Parent = Player.Character:FindFirstChild("Right Leg")
        -- Left Leg
        local SBox5 = INew("SelectionBox", workspace)
        SBox5.LineThickness = 0.001
        SBox5.Transparency = 0.1
        SBox5.Color3 = Color3.new(245,245,245)
        SBox5.Adornee = Player.Character:FindFirstChild("Left Leg")
        SBox5.Parent = Player.Character:FindFirstChild("Left Leg")
    end
end

getgenv().EspCoolKid = function(COolPlayer)
    local bgui = INew("BillboardGui", COolPlayer.Character:WaitForChild("Head", 10))
    local tlab = INew("TextLabel", bgui)
    bgui.Adornee = COolPlayer.Character.Head
    bgui.Size = UD2(0,100,0,100)
    bgui.StudsOffset = Vec3(0,1,0)
    bgui.AlwaysOnTop = true 
    tlab.BackgroundTransparency = 1
    tlab.Position = UD2(0,0,0,1)
    tlab.Size = UD2(1,0,0,10)
    tlab.TextStrokeTransparency = 0.5
    tlab.TextSize = 10
    tlab.TextStrokeColor3 = BackDoorUsersTable[COolPlayer.UserId].COLOUR
    tlab.TextColor3 = BackDoorUsersTable[COolPlayer.UserId].COLOUR
    tlab.Text = BackDoorUsersTable[COolPlayer.UserId].NAME
end

getgenv().Esp = function(Dude)
    local UserFound = UserTable[Dude] and "(Yes)" or "(No)"
    local bgui = Instance.new("BillboardGui", Dude.Character.Head)
    local tlabel = Instance.new("TextLabel", bgui)
    
    bgui.Name = "ESP"
    bgui.Adornee = part
    bgui.AlwaysOnTop = true
    bgui.ExtentsOffset = Vector3.new(0, 3, 0)
    bgui.Size = UDim2.new(0, 5, 0, 5)
        
    tlabel.Name = "espTarget"
    tlabel.BackgroundColor3 = Color3.new(255, 255, 255)
    tlabel.BackgroundTransparency = 1
    tlabel.BorderSizePixel = 0
    tlabel.Position = UDim2.new(0, 0, 0, -30)
    tlabel.Size = UDim2.new(1, 0, 7, 0)
    tlabel.Visible = true
    tlabel.ZIndex = 10
    tlabel.Font = "ArialBold"
    tlabel.FontSize = "Size18"
    RService.RenderStepped:Connect(function()
        if Dude and Dude.Character and Dude.Character:FindFirstChildOfClass("Humanoid") then 
            tlabel.Text = Dude.Name.." | User: "..UserFound.." | Health: (".. math.floor((Dude.Character.Humanoid.Health))..")".." | ForceFielded: "..Values(Dude, "ForceField").." | Glock: "..Items(Dude,"Glock").." | Shotty: "..Items(Dude, "Shotty").." | Vest: "..Items(Dude, "BulletResist")
        end
    end)
    tlabel.TextColor = BrickColor.new("White") 
    tlabel.TextStrokeTransparency = 0.5
    CreatePartEsp(Dude)
    
    Dude.CharacterAdded:Connect(function()
        wait(0.2)
        local bgui = Instance.new("BillboardGui", Dude.Character.Head)
        local tlabel = Instance.new("TextLabel", bgui)
        
        bgui.Name = "ESP"
        bgui.Adornee = part
        bgui.AlwaysOnTop = true
        bgui.ExtentsOffset = Vector3.new(0, 3, 0)
        bgui.Size = UDim2.new(0, 5, 0, 5)
        
        tlabel.Name = "espTarget"
        tlabel.BackgroundColor3 = Color3.new(255, 255, 255)
        tlabel.BackgroundTransparency = 1
        tlabel.BorderSizePixel = 0
        tlabel.Position = UDim2.new(0, 0, 0, -30)
        tlabel.Size = UDim2.new(1, 0, 7, 0)
        tlabel.Visible = true
        tlabel.ZIndex = 10
        tlabel.Font = "ArialBold"
        tlabel.FontSize = "Size18"
        RService.RenderStepped:Connect(function()
            if Dude and Dude.Character and Dude.Character:FindFirstChildOfClass("Humanoid") then 
                tlabel.Text = Dude.Name.." | User: "..UserFound.." | Health: (".. math.floor((Dude.Character.Humanoid.Health))..")".." | ForceFielded: "..Values(Dude, "ForceField").." | Glock: "..Items(Dude,"Glock").." | Shotty: "..Items(Dude, "Shotty").." | Vest: "..Items(Dude, "BulletResist")
            end
        end)
        tlabel.TextColor = BrickColor.new("White") 
        tlabel.TextStrokeTransparency = 0.5
        CreatePartEsp(Dude)
    end)
end

getgenv().UnEsp = function(Dude2)
    for _, v in pairs(Dude2.Character:GetDescendants()) do 
        if v:IsA("TextLabel") or v:IsA("BillboardGui") or v.Name == "SelectionBox" then 
            v:Destroy()
        end
    end
    Dude2.CharacterAdded:Connect(function()
        for _, v in pairs(Dude2.Character:GetDescendants()) do 
            if v:IsA("TextLabel") or v:IsA("BillboardGui") or v.Name == "SelectionBox" then 
                v:Destroy()
            end
        end
    end)
end

getgenv().PrintCommands = function()
    rconsolename("Citizen Commands (Do Not Close)")
    rconsoleprint("@@RED@@")
    rconsolewarn("Citizen Commands, If you close this window your game will crash!")
    rconsolewarn([[
    
    
    Citizen V4, Made by xaxa/zxciaz and slieepy.
    Current Version: 4.1 (12/14/2020)
    Discord Server: discord.gg/qF6jbZzPE3
    ===================
    Keybinds (Changable)
    ====================
    {Key Section}:
    F, Toggles Fly 
    X, Toggles NoClip
    T, Spits Random Trash-Talk
    P, Resets your Character
    N, Buys Ammo (Hold)
    M, Buyz Uzi (Hold)
    B, Buys Glock (Hold)
    V, Buys Burger/Food (Hold)
    E, Shoots Aimlock-Target (PRISON ONLY)
    Q, Selects Aimlock-Target (Hover mouse over Player then press key)
    Left-Click, Shoots Aimlock-Target (STREETS ONLY, PRESS E IF YOUR IN PRISON)
    Left-Shift, Blinkspeed (MAKE SURE SHIFT-LOCK IS OFF)
    Semicolon, Opens Command Bar 
    ========================
    Commands ("." in Chat for prefix (example: ".fspeed 10"), No prefix for command bar)
    CommandName - Other Names - Type of Word - Description
    =========================================================
    
    {Combat Section}:
    legitmode, Legit Mode On (Disables flykeys n stuff)
    hvhmode, Turns off Legtimode so you can have flykeys back n stuff
    
    feloop [fekill, fe] <namehere>, Sets Feloop Target 
    unfeloop [unfekill, unfe], Turns off Feloop
    
    aimlock [aim, al, a] <name>, Sets Aimlock-Target
    unaimlock [unaim, unal, una], Toggles Aimlock
    aimpart [] <head,torso,toes>, Sets Aim-Target Part
    aimmode [old, new], New = Left Click To Aimbot, Old = Hold/Press E to Aimbot
    
    camlock [cam, cl, c] <name>, Sets Camlock-Target
    uncamlock [uncam, uncl, unc], Toggles Camlock
    campart [] <head,torso,toes>, Sets Cam-Target Part
    
    shottyfollow [sfollow, sk] <name>, Sets Shotty-Follow Target (Prison Only)
    glockfollow [gfollow, gk] <name>, Sets Glock-Follow Target (Prison Only)
    uzifollow [ufollow, uk] <name>, Sets Uzi-Follow Target (Streets Only)
    
    manualbot [mbot, m] <name>, Sets Manual-Triggerbot Target (Prison Only) (TPBypass Suggested)
    esp [trace, find, locate] <name>, Sets ESP-Target
    unesp [untrace, unfind, unlocate] <name>, UnEsp's Target 
    goto [tpto, to] <name>, Teleports you to Player
    
    spam <wordhere>, Starts Chat-Spamming the word 
    checkscript <name>, Tells you if the Target is using CyAdmin or DrPoppa or Citizen
    
    ------------------
    {Movement Section}:
    walkspeed [ws] <number>, Sets your WalkSpeed (Prison Only)
    jumppower [jp] <number>, Sets your JumpPower (Prison Only)
    hipheight [hh] <number>, Sets your HipHeight (Prison Only)
    flyspeed [fspeed, fs] <number>, Sets your flyspeed
    blinkspeed [bs], Sets Blinkspeed 
    fieldofview [fov], Sets your FOV 
    
    antife [], Toggles Anti Feloop/Van/Kill
    noslow [ns], Toggles Noslow (Buggy) (Prison Only)
    infinitejump [infjump], Toggles Infjump 
    blink, Toggles Blink (Shift-Lock must be off)
    teleportbypass [tpbypass, bypass], Toggles Teleport-Bypass
    antilog [], Inject Dot_MP4's Anti-Audio Logger 
    itemesp [], Toggles Item-Esp
    gunanims [ganims], Toggles Gun-Animations
    punchfling [pfling, pf], Flings someone when you Punch them (Equips Punch for you)
    
    novan, Deletes the Van 
    banland, Teleports you to Prison 
    severhop [shop], Joins a different server in your current game
    
    -------------------------
    get <uzi,glock,pipe,sawoff,burger,chicken>, Teleports you to which one you pick and buys it for you 
    
    -------------------------
    {Hotkey Changing Section}:
    prefix [] <word>, Sets your prefix for chat commands, New Example: (\flyspeed 5) -- only if you set your prefix as "\"
    flykey [fkey] <key>, Sets your Toggle-Fly Key 
    noclipkey [nckey] <key>, Sets your NoClip Key
    trashtalkkey [ttkey] <key>, Sets your Trash-Talk Key 
    shootlockedkey [slockkey] <key>, Sets your Shoot Aimlock Key (PRISON ONLY)
    
    buyuzikey [uzikey] <key>, Sets your Buy-Uzi Key (STREETS ONLY)
    buyammokey [ammokey] <key>, Sets your Buy-Ammo Key (STREETS ONLY)
    buyglockkey [glockkey] <key>, Sets your Buy-Glock Key (STREETS ONLY)
    buyfoodkey [foodkey] <key>, Sets your Buy-Burger Key (STREETS ONLY) 
    
    --------------------
    {Visualizer Section}:
    printsettings, Prints your settings for the visualizer 
    vistool <uzi,glock, etc.>, Sets your Visualizer Tool 
    xoffset [xset], Sets the X-Angle for the visualizer
    yoffset [xset], Sets the Y-Angle for the visualizer
    zoffset [zset], Sets the Z-Angle for the visualizer
    ooffset [oset], Sets the Tilt-Angle for the visualizer
    ]])
    print([[
    
    Citizen V4, Made by xaxa/zxciaz and slieepy.
    Current Version: 4.1 (12/14/2020)
    Discord Server: discord.gg/qF6jbZzPE3
