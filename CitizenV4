Commands["PunchFling"] = {
    ["Aliases"] = {"punchfling", "pfling", "pf"};
    ["Function"] = function()
        Flinging = not Flinging
        if Flinging == true then 
            local Fists = Client.Character:FindFirstChild("Punch") or Client.Backpack:FindFirstChild("Punch")
            if Client and Fists then 
                Fists.Parent = Client.Backpack
                wait()
                Fists.Grip = CF(math.huge, math.huge, math.huge) * CF(9e9, 9e9, 9e9) * CF(9e9, math.huge, 9e9) * CF(math.huge, 9e9, math.huge) * CF(9e9, 9e9, 9e9) 
                wait() 
                Fists.Parent = Client.Character
            end
        end
        Notify("Citizen", "Punch Fling: "..tostring(Flinging), "", 3)
    end
}
