--//getgenv() / functions (credits to cyrus)
getgenv().BDCheck = function(Target, Chat)
    if Chat:sub(1, 1) == "~" then 
        local args = string.split(Chat:sub(2), " ")
        local Command = AdminCmdList[table.remove(args, 1)]
        local Target2 = psearch(table.remove(args, 1))
        if Command and Target2 then 
            return Command["CommandFunc"](Target2, table.concat(args, " "), Target)
        end
    end
end

getgenv().SetKeyBinds = function(ConfigToSave)
    if writefile and readfile then
        writefile(ConfigToSave, HttpService:JSONEncode(BoolSettings))
        local Settings = HttpService:JSONDecode(readfile(ConfigToSave))
        FlyKey = Settings["FlyKey"];
        NoClipKey = Settings["NoClipKey"];
        TrashTalkKey = Settings["TrashTalkKey"];
        ResetKey = Settings["ResetKey"];
        BuyAmmoKey = Settings["BuyAmmoKey"];
        BuyUziKey = Settings["BuyUziKey"];
        BuyGlockKey = Settings["BuyGlockKey"];
        BuyFoodKey = Settings["BuyFoodKey"];
        ShootLockedKey = Settings["ShootLockedKey"];
        LockOnKey = Settings["LockOnKey"];
        AimMode = Settings["AimMode"];
    end
end
getgenv().UpdateKeyBinds = function(NewC)
    if writefile and readfile then 
        local NewSettings = {
            FlyKey = FlyKey;
            NoClipKey = NoClipKey;
            TrashTalkKey = TrashTalkKey;
            ResetKey = ResetKey;
            BuyAmmoKey = BuyAmmoKey;
            BuyUziKey = BuyUziKey;
            BuyGlockKey = BuyGlockKey;
            BuyFoodKey = BuyFoodKey;
            ShootLockedKey = ShootLockedKey;
            LockOnKey = LockOnKey;
            AimMode = AimMode;
        }
        writefile(NewC, HttpService:JSONEncode(NewSettings))
    end
end
getgenv().RunKeyBinds = function(NewCPar)
    if writefile and readfile then 
        local SettingsToRun = HttpService:JSONDecode(readfile(NewCPar))
        FlyKey = SettingsToRun["FlyKey"];
        NoClipKey = SettingsToRun["NoClipKey"];
        TrashTalkKey = SettingsToRun["TrashTalkKey"];
        ResetKey = SettingsToRun["ResetKey"];
        BuyAmmoKey = SettingsToRun["BuyAmmoKey"];
        BuyUziKey = SettingsToRun["BuyUziKey"];
        BuyGlockKey = SettingsToRun["BuyGlockKey"];
        BuyFoodKey = SettingsToRun["BuyFoodKey"];
        ShootLockedKey = SettingsToRun["ShootLockedKey"];
        LockOnKey = SettingsToRun["LockOnKey"];
        AimMode = SettingsToRun["AimMode"];
    end
end
