Commands["Antife"] = {
    ["Aliases"] = {"antife"};
    ["Function"] = function()
        AntiFe = not AntiFe
        if AntiFe == true then 
            RealTools = Client.Backpack:GetChildren()
            for _, v in next, RealTools do 
                if v:IsA("Tool") then 
                    local TF = INew("Folder", v.Handle)
                    TF.Name = "=hash&&niljavaencode&&andhash=nil+true"
                end
            end
            Client.Character.ChildAdded:Connect(function(T1)
                if not T1.Handle:FindFirstChild("=hash&&niljavaencode&&andhash=nil+true") then 
                    T1:Destroy()
                end
                if table.find(RealTools, T1) then 
                    return wait(9e9)
                elseif not table.find(RealTools, T1) then 
                    T1:Destroy();T1.Parent = nil 
                end
            end)
            Client.Backpack.ChildAdded:Connect(function(T2)
                if not T2.Handle:FindFirstChild("=hash&&niljavaencode&&andhash=nil+true") then 
                    T2:Destroy()
                end
                if table.find(RealTools, T2) then 
                    return wait(9e9)
                elseif not table.find(RealTools, T2) then 
                    T2:Destroy();T2.Parent = nil 
                end
            end)
        end
        Notify("Citizen", "Antife: "..tostring(AntiFe), "", 3)
    end
}

Commands["GunAnims"] = {
    ["Aliases"] = {"gunanims", "ganims"};
    ["Function"] = function()
        Animating = not Animating
        if Animating == true then 
            local Anim = INew("Animation");Anim.AnimationId = "rbxassetid://889968874"
            local AnimP = LocalP():FindFirstChild("Humanoid"):LoadAnimation(Anim)
            LocalP().ChildAdded:Connect(function(T3)
                AnimP:Play()
            end)
        elseif Animating == false then 
            AnimP:Stop()
        end
        Notify("Citizen", "Gun Anims: "..tostring(Animating), "", 3)
    end
}
Commands["ItemEsp"] = {
    ["Aliases"] = {"itemesp"};
    ["Function"] = function()
        loadstring(game:HttpGet("https://pastebin.com/raw/Wzv2ftY6", true))()
    end
}

Commands["WalkSpeed"] = {
    ["Aliases"] = {"walkspeed", "ws"};
    ["Function"] = function(args)
        if args[1] then
            if Client and Client.Character and Client.Character:FindFirstChild("Humanoid") then 
                Client.Character.Humanoid.WalkSpeed = tonumber(args[1])
            end
        end
        Notify("Citizen", "WalkSpeed: "..tonumber(args[1]), "", 3)
    end
}
