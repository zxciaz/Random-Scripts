Commands["MBot"] = {
    ["Aliases"] = {"manualbot", "mbot", "m"};
    ["Function"] = function(args)
        if args[1] then 
            getgenv().MBotTarget = psearch(args[1]);AimlockTarget = MBotTarget
            if MBotTarget and MBotTarget ~= nil or MBotTarget ~= Client then 
                repeat RService.Heartbeat:Wait()
                    MBotPlayer(MBotTarget);PShootTarget(MBotTarget)
                until MBotTarget.Character:FindFirstChild("KO") or MBotTarget.Character.Humanoid.Health == 0 or Client.Character:FindFirstChild("KO") or Client.Character.Humanoid.Health == 0 
                if MBotTarget.Character:FindFirstChild("KO") or MBotTarget.Character.Humanoid.Health == 0 or Client.Character:FindFirstChild("KO") or Client.Character.Humanoid.Health == 0 then 
                    MBotTarget = nil
                end
            end
        end
        Notify("Citizen", "Manual-Bot Target: "..tostring(MBotTarget), "", 3)
    end
}
Commands["UziFollow"] = {
    ["Aliases"] = {"uzifollow", "ufollow", "uk"};
    ["Function"] = function(args)
        if args[1] then 
            getgenv().UKTarget = psearch(args[1])
            if UKTarget and UKTarget ~= nil then 
                AimlockTarget = UKTarget;setsimulationradius(math.huge, math.huge)
                if Client.Backpack:FindFirstChild("Uzi") then 
                    Client.Backpack.Uzi.Parent = Client.Character 
                    local Uzi = Client.Character:FindFirstChild("Uzi")
                        local RP = INew("RocketPropulsion", Uzi.Handle)
                        RP.ThrustP = 20000;RP.TurnP = 2000;RP.MaxSpeed = 10000;RP.MaxThrust = 200000;RP:Fire()
                        while UKTarget and UKTarget.Character and UKTarget.Character:FindFirstChild(AimPart) and Uzi and RP do 
                            Client.Character["Right Arm"]:FindFirstChild("RightGrip"):Destroy()
                            repeat RService.Heartbeat:Wait() 
                                RP.Target = UKTarget.Character.Head
                            until UKTarget.Character:FindFirstChild("Bone", true) or UKTarget.Character:FindFirstChild("KO") or UKTarget.Character.Humanoid.Health == 0 or Client.Character:FindFirstChild("Bone", true) or Client.Character:FindFirstChild("KO") or Client.Character.Humanoid.Health == 0
                            if UKTarget.Character:FindFirstChild("Bone", true) or UKTarget.Character:FindFirstChild("KO") or UKTarget.Character.Humanoid.Health == 0 or Client.Character:FindFirstChild("Bone", true) or Client.Character:FindFirstChild("KO") or Client.Character.Humanoid.Health == 0 then 
                                UKTarget = nil;RP:Destroy()
                            end
                        end
                    end
                end
            end
        Notify("Citizen", "Uzi-Follow Target: "..tostring(UKTarget), "", 3)
    end
}
Commands["GlockFollow"] = {
    ["Aliases"] = {"glockfollow", "gfollow", "gk"};
    ["Function"] = function(args)
        if args[1] and game.PlaceId ~= (StreetsID) then 
            getgenv().GKTarget = psearch(args[1])
            if GKTarget and GKTarget ~= nil then 
                AimlockTarget = GKTarget;setsimulationradius(math.huge, math.huge)
                if Client.Backpack:FindFirstChild("Glock") then 
                    Client.Backpack.Glock.Parent = Client.Character 
